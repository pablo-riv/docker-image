namespace :commune do
  desc 'update available communes'
  task availables: :environment do
    communes = ['Las Condes','Providencia', 'Santiago Centro']
    communes.each do |c|
      commune = Commune.find_by(name: c.upcase)
      commune.update_columns(is_available: true) if commune.present?
    end
    puts "Communes availables updated!"
  end

  desc 'Include postal code to commune'
  task postal_code: :environment do
    csv = File.read("#{Rails.root}/lib/data/postal_code.csv")
    remove_characters = lambda do |commune|
      commune.tr("ÀÁÃÄÅàáâãäåĀāĂăĄąÇçĆćĈĉĊċČčÐðĎďĐđÈÉÊËèéêëĒēĔĕĖėĘęĚěĜĝĞğĠġĢģĤĥĦħÌÍÎÏìíîïĨĩĪīĬĭĮįİıĴĵĶķĸĹĺĻļĽľĿŀŁÑñŃńŅņŇňŉŊŋÒÓÔÕÖØòóôõöøŌōŎŏŐőŔŕŖŗŘřŚśŜŝŞşŠšſŢţŤťŦŧÙÚÛÜùúûüŨũŪūŬŭŮůŰűŲųŴŵÝýÿŶŷŸŹźŻżŽž","AAAAAaaaaaaAaAaAaCcCcCcCcCcDdDdDdEEEEeeeeEeEeEeEeEeGgGgGgGgHhHhIIIIiiiiIiIiIiIiIiJjKkkLlLlLlLlLNnNnNnNnnNnOOOOOOooooooOoOoOoRrRrRrSsSsSsSssTtTtTtUUUUuuuuUuUuUuUuUuUuWwYyyYyYZzZzZz")
    end

    CSV.parse(csv, headers: true).each_with_index do |data, _index|
      data = data.to_hash.with_indifferent_access
      commune = Commune.where("LOWER(name) LIKE ?", "%#{remove_characters.call(data[:name]).downcase}%").first
      unless commune.present?
        puts "ERROR NOT FOUND #{remove_characters.call(data[:name]).downcase}".red.swap
        next
      end

      commune.update_columns(code: data[:code])
      puts "COMMUNE UPDATED #{data[:name]}".green.swap
    end
  end

  desc 'Add new courier to configuration'
  task :add_courier, [:name, :reference_courier_name] => :environment do |_t, args|
    puts 'Adding new courier to communes'.cyan.swap
    new_courier_name = args[:name]&.strip&.downcase
    reference_courier_name = args[:reference_courier_name]&.strip&.downcase
    Commune.all.each do |commune|
      commune.couriers_availables[new_courier_name] = commune.couriers_availables[reference_courier_name]
      puts "#{commune.name}: #{commune.couriers_availables[new_courier_name]}".yellow.swap
      commune.update_columns(couriers_availables: commune.couriers_availables.with_indifferent_access)
    end
    puts "New courier #{new_courier_name} added!".green.swap
  end

  desc 'Remove dhl and cc from communes'
  task remove_couriers_from_communes: :environment do
    puts 'Init process'.green
    puts '======================================='.green
    Commune.all.each do |commune|
      couriers = commune.couriers_availables
      puts "#{couriers}".red
      couriers.reject! {|courier| courier['dhl'] || courier['correoschile']}
      puts "#{couriers}".blue
      commune.update_columns(couriers_availables: couriers)
    end
  end

