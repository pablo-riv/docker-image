<div class="main-content">
  <div class="content-view">
    <div class="layout-md b-b">
      <div class="layout-column-md">
        <div class="row">
          <div class="col-lg-12 col-sm-12">
            <div class="card">
              <div class="card-block">
                <% if @type.include?("company") %>
                  <%= render "form_company_accounts" %>
                <% else %>
                <%= render "form_branch_office_accounts" %>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
